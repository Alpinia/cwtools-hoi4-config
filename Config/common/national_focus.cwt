		will_lead_to_war_with = scope[country]
		## cardinality = 0..50
		prerequisite = {
			## cardinality = 1..50
			focus = <focus>
		}
		## cardinality = 0..1
		mutually_exclusive = {
			## cardinality = 1..50
			focus = <focus>
		}
		## cardinality = 0..1
		bypass = {
			alias_name[trigger] = alias_match_left[trigger]
		}
		## cardinality = 0..1
		allow_branch = {
			alias_name[trigger] = alias_match_left[trigger]
		}
		## cardinality = 0..1
		available = {
			alias_name[trigger] = alias_match_left[trigger]
		}
		## cardinality = 0..1
		cancel = {
			alias_name[trigger] = alias_match_left[trigger]
		}
		## cardinality = 0..1
		historical_ai = {
			alias_name[trigger] = alias_match_left[trigger]
		}
		## cardinality = 0..1
		select_effect = {
			alias_name[effect] = alias_match_left[effect]
		}
		## cardinality = 0..1
		complete_tooltip = {
			alias_name[effect] = alias_match_left[effect]
