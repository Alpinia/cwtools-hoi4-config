types = {
	type[focus_tree] = {
		type_key_filter = focus_tree
		path = "game/common/national_focus"
		name_field = "id"
	}
	type[focus] = {
		path = "game/common/national_focus"
		skip_root_key = focus_tree
		type_key_filter = focus
		name_field = "id"
	}
	type[shared_focus] = {
		path = "game/common/national_focus"
		name_field = "id"
		type_key_filter = shared_focus
	}
}

focus_tree = {
	id = localisation
	country = {
		factor = int
		## cardinality = 0..10
		modifier = {
			add = int
			alias_name[trigger] = alias_match_left[trigger]
		}
		## cardinality = 0..10
		modifier = {
			factor = int
			alias_name[trigger] = alias_match_left[trigger]
		}
	}
	default = bool
	## cardinality = 0..10
	shared_focus = <shared_focus>
	## cardinality = 1..1000
	focus = {
		id = localisation
		## cardinality = 0..1
		text = localisation
		icon = scalar
		cost = float
		x = int
		y = int
		## cardinality = 0..1
		relative_position_id = <focus>
